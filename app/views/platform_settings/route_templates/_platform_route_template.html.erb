<%= content_tag :tr, id: dom_id(platform_route_template) do %>
  <%= render "shared/table/table_data_cell", data: platform_route_template.description %>
  <%= render "shared/table/table_data_cell", data: platform_route_template.route_no %>
  <%= render "shared/table/table_data_cell", data: ldate(platform_route_template.next_planned_date, format: :default) %>
  <%= render "shared/table/table_data_cell", data: platform_route_template.is_deleted %>    
  <%= render "shared/table/table_data_cell", data: platform_route_template.guid %>    
<% end %>