<%= content_tag :tr, id: dom_id(platform_accounting_period) do %>
  <%= render "shared/table/table_data_cell", data: platform_accounting_period.description %>
  <%= render "shared/table/table_data_cell", data: platform_accounting_period.start_date %>
  <%= render "shared/table/table_data_cell", data: platform_accounting_period.end_date %>
  <%= render "shared/table/table_data_cell", data: platform_accounting_period.is_closed %>    
  <%= render "shared/table/table_data_cell", data: platform_accounting_period.guid %>    
<% end %>